    "type": "git",
    "url": "https://github.com/IDuxFE/archive",
    "directory": "packages/archive-app"
  },
  "dependencies": {
    "@idux/archive-utils": "^0.2.2"
  },
  "devDependencies": {
    "@idux/archive-types": "^0.2.2",
    "@idux/archive-vite-markdown-plugin": "^0.2.2",
    "@idux/archive-vite-plugin": "^0.2.2",
    "@idux/cdk": "^1.3.0",
    "@idux/components": "^1.3.0",
    "@idux/pro": "^1.3.0",
    "@types/lodash-es": "^4.17.6",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitejs/plugin-vue-jsx": "^2.1.1",
    "less": "^4.1.3",
    "lodash-es": "^4.17.21",
    "rollup": "^3.4.0",
    "vite": "^3.2.3",
    "vue": "^3.2.41",
    "vue-router": "^4.1.6"
  }
}
