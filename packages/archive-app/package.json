{
  "name": "@idux/archive-app",
  "version": "0.3.2",
  "description": "",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./vue": {
      "import": "./dist/vue.js",
      "types": "./dist/vendor/vue.d.ts"
    },
    "./components": {
      "import": "./dist/components.js",
      "types": "./dist/vendor/components.d.ts"
    },
    "./createDemoInstance": {
      "import": "./dist/createDemoInstance.js",
      "types": "./dist/createDemoInstance.d.ts"
    },
    "./bundle": {
      "import": "./bundle"
    },
    "./*": [
      "./*",
      "./*.d.ts"
    ]
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/vendor/*",
        "./*"
      ]
    }
  },
  "files": [
    "bundle",
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf dist && pnpm run build:app && pnpm run build:style && pnpm run build:types",
    "build:app": "vite build",
    "build:style": "rimraf dist/app/themes && node ./scripts/buildStyle.js",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir ./dist"
  },
  "author": "IDuxFE<idux.feteam@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/IDuxFE/archive",
    "directory": "packages/archive-app"
  },
  "dependencies": {
    "@idux/archive-loader-vue": "0.3.2",
    "@idux/archive-utils": "^0.3.1"
  },
  "devDependencies": {
    "@idux/archive-types": "^0.3.1",
    "@idux/archive-vite-markdown-plugin": "^0.3.0",
    "@idux/archive-vite-plugin": "^0.3.1",
    "@idux/cdk": "1.5.0",
    "@idux/components": "1.5.1",
    "@idux/pro": "1.5.1",
    "@types/lodash-es": "^4.17.6",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitejs/plugin-vue-jsx": "^2.1.1",
    "less": "^4.1.3",
    "lodash-es": "^4.17.21",
    "rollup": "^3.4.0",
    "vite": "^4.3.9",
    "vue": "^3.2.41",
    "vue-router": "^4.1.6"
  }
}
